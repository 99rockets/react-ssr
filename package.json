{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "React application",
  "private": true,
  "author": "99rockets",
  "license": "MIT",
  "scripts": {
    "audit": "npm audit",
    "clean": "rm -rf static dist node_modules/.cache",
    "build": "npm run clean && NODE_ENV=production webpack --config webpack/prod && tsc",
    "dev": "NODE_ENV=development nodemon -e ts -w src/server -x \"node -r ts-node/register src/server/app/http.ts\"",
    "start": "NODE_ENV=production node dist/server/app/http.js",
    "lint": "npm run lint:eslint && npm run lint:ts",
    "lint:eslint": "eslint . --cache",
    "lint:ts": "tsc --noEmit",
    "lint:css": "stylelint 'src/**/*.@(ts|tsx)'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Express",
    "React",
    "SSR",
    "Isomorphic"
  ],
  "engines": {
    "node": "17.4.0",
    "npm": "8.3.1"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@reduxjs/toolkit": "^1.7.1",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.20.3",
    "express": "^4.17.2",
    "helmet": "^5.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.2.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "serialize-javascript": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@emotion/babel-plugin": "^11.7.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/serialize-javascript": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "assets-webpack-plugin": "^7.1.1",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.2.2",
    "eslint": "^8.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.28.0",
    "file-loader": "^6.2.0",
    "nodemon": "^2.0.15",
    "postcss-styled": "^0.34.0",
    "react-refresh": "^0.11.0",
    "stylelint": "^14.3.0",
    "terser-webpack-plugin": "^5.3.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-middleware": "^5.3.0",
    "webpack-hot-middleware": "^2.25.1",
    "webpack-merge": "^5.8.0"
  }
